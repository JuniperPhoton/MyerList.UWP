    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:base="using:MyerList.Base"
    xmlns:uc="using:MyerList.UC"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{StaticResource MyerListBlue}">

    <Grid Background="White">
        <uc:CatePersonalizationControl OnClickCancelBtn="CatePersonalizationControl_OnClickCancelBtn" OnClickOKBtn="CatePersonalizationControl_OnClickOKBtn"/>
    </Grid>
</base:CustomTitleBarPage>
